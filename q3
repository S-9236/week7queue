// 3. Program to implement a circular queue
#include <stdio.h>
#define MAX 5
int cqueue[MAX];
int front = -1, rear = -1;
void enqueue(int x) {
    if ((front == 0 && rear == MAX - 1) || (rear + 1 == front))
        printf("Queue Overflow\n");
    else {
        if (front == -1) front = 0;
        rear = (rear + 1) % MAX;
        cqueue[rear] = x;
    }
}
void dequeue() {
    if (front == -1)
        printf("Queue Underflow\n");
    else {
        printf("Dequeued: %d\n", cqueue[front]);
        if (front == rear)
            front = rear = -1;
        else
            front = (front + 1) % MAX;
    }
}
void display() {
    if (front == -1)
        printf("Queue is empty\n");
    else {
        int i = front;
        while (1) {
            printf("%d ", cqueue[i]);
            if (i == rear) break;
            i = (i + 1) % MAX;
        }
        printf("\n");
    }
}
int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    display();
    dequeue();
    enqueue(40);
    enqueue(50);
    enqueue(60);
    display();
    return 0;
}
