// 7. Program to check overflow and underflow conditions in queue
#include <stdio.h>
#define MAX 3
int queue[MAX];
int front = -1, rear = -1;
void enqueue(int x) {
    if (rear == MAX - 1)
        printf("Overflow: Queue is full\n");
    else {
        if (front == -1) front = 0;
        queue[++rear] = x;
    }
}
void dequeue() {
    if (front == -1 || front > rear)
        printf("Underflow: Queue is empty\n");
    else
        front++;
}
int main() {
    dequeue();
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    dequeue();
    dequeue();
    dequeue();
    dequeue();
    return 0;
}
