// 6. Program to perform enqueue and dequeue operations and display after each operation
#include <stdio.h>
#define MAX 5
int queue[MAX];
int front = -1, rear = -1;
void enqueue(int x) {
    if (rear == MAX - 1)
        printf("Overflow\n");
    else {
        if (front == -1) front = 0;
        queue[++rear] = x;
        printf("Enqueued: %d\n", x);
    }
}
void dequeue() {
    if (front == -1 || front > rear)
        printf("Underflow\n");
    else
        printf("Dequeued: %d\n", queue[front++]);
}
void display() {
    if (front == -1 || front > rear)
        printf("Queue is empty\n");
    else {
        for (int i = front; i <= rear; i++)
            printf("%d ", queue[i]);
        printf("\n");
    }
}
int main() {
    enqueue(10); display();
    enqueue(20); display();
    enqueue(30); display();
    dequeue(); display();
    enqueue(40); display();
    return 0;
}
